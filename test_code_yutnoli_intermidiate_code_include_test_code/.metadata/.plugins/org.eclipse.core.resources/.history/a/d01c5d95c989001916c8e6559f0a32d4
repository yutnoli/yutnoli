package test.unit;

import Model.PlayerData;
import org.junit.After;
import org.junit.Before;
import org.junit.Test;
import static org.junit.Assert.assertEquals;
import junit.framework.JUnit4TestAdapter;

public class TestStateData {
	private PlayerData playerData; // test fixture variable.
	
	/* setUp method using @Before syntax. */
	/* @Before methods are run before each test. */
	/* initializes text feature variables. */
	@Before public void runBeforefindMoveTest(){
		System.out.println("Before testing.");
		playerData = new PlayerData();
	}
	
	@After public void runAfterfindMoveTest() {
		System.out.println("After testing.");
		
		/* execute garbage collection. */
		System.gc();
		playerData = null;
		
	}
	
}
