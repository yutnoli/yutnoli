package View;

import java.awt.*;

import javax.swing.*;

public class MapPrint extends JFrame {
	JPanel statePanel = new JPanel();
	JButton normal_state;
	
	public void testImage() {
		this.setTitle("Yutnoli");
		this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		normal_state = new JButton(new ImageIcon("C:\\Users\\Administrator\\Desktop\\test\\TestView\\normal_state.png"));
//		normal_state.setBorderPainted(false);;
//		normal_state.setContentAreaFilled(false);
//		normal_state.setFocusPainted(false);
		Container gamePanel = getContentPane();
//		c.setLayout(null);
		gamePanel.setLayout(null);
		statePanel.setLayout(null);
		statePanel.setLocation(100, 100);
		statePanel.setSize(50, 50);
		statePanel.setBackground(Color.black);
		statePanel.add(normal_state);
		gamePanel.add(statePanel);
//		normal_state.setLayout(null);
//		normal_state.setLocation(100, 100);
//		normal_state.setSize(50, 50);
//		gamePanel.add(normal_state);
		gamePanel.setBackground(Color.white);
		this.setSize(1000, 1000);
		this.setLocationRelativeTo(null);
		this.setVisible(true);
	}
}


